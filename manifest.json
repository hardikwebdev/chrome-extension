{
  "name": "Chrome Extension ",
  "description": "Chrome Extension Description",
  "version": "1.0.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "/js/settings.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": [
    "*://*.URL.COM//"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "/images/16.png",
        "32": "/images/32.png",
        "48": "/images/48.png",
        "128": "/images/120.png"
    }
  },
  "icons": {
    "16": "/images/16.png",
    "32": "/images/32.png",
    "48": "/images/48.png",
    "128": "/images/120.png"
  },
  "options_page": "options.html"
}
